{
  "name": "iotstarter",
  "version": "1.0.0",
  "description": "IoT starter pack for NodeJS\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[Dusing NodeJS",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && cross-env GENERATE_SOURCEMAP=false && npm run build --prefix client"
  },
  "keywords": [
    "IoT",
    "MQTT",
    "API",
    "Express",
    "HTTP"
  ],
  "author": "JM VALINO",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.1",
    "morgan": "^1.9.0",
    "mqtt": "^2.18.2",
    "mysql2": "^1.6.1",
    "nodemon": "^1.17.5"
  }
}
